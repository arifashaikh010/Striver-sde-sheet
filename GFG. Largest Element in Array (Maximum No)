class Compute {
    
    public int largest(int arr[], int n)
    {
        int ans= Integer.MIN_VALUE;
        //int ans= arr[0];                                                            // striver
        for(int i=0;i<n;i++){
            if(ans<arr[i]){
                ans=arr[i];
            }
        }
        return ans;
    }
}

/*
Input:
n = 5
A[] = {1, 8, 7, 56, 90}
Output:
90

Input:
n = 7
A[] = {1, 2, 0, 3, 2, 4, 5}
Output:
5
*/
