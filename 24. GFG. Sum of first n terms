class Solution {
    long ans=0;
    long sumOfSeries(long N) {
        if(N<1){
            return ans;
        }
        ans=ans+(N*N*N); 
        sumOfSeries(N-1);
        return ans;
    }
}

//-------------------------------------------------- OR -------------------------------------------------------------

class Solution {
    long sumOfSeries(long N) {
        if(N==1){
            return 1;
        }
        return N*N*N+sumOfSeries(N-1);
    }
}

